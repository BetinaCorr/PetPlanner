<h1>Create a new Professional</h1>

<div>
  <br>
      <%= simple_form_for [@pet, @professional] do |f| %>
      <%= f.input :name, :label => 'Nome' %>
      <%= f.input :address, :label => 'Endereço' %>
       <%= f.input :phone_number, :label => 'Telemóvel' %>
      <%= f.button :submit, "Add this professional to your pet" %>
    <% end %>
</div>
